create table loja_de_suplementos (
nome varchar(100) not null,
suplementos int not null,
CNPJ_lojasuplementos varchar(100) not null,
primary key (CNPJ_lojasuplementos)
  )
  
 create table creatina (
  peso int not null,
  monohidratada BOOLEAN not null,
  sabor varchar(100) not null,
   marca_creatina varchar(100) not null,
   primary key (marca_creatina)
   )
   
  create table wheyprotein(
  peso int not null,
  quantidade_proteina int not null,
  marca_whey VARCHAR(100) not null,
  primary key (marca_whey)
  )
